{
    "name": "ibuildings/qa-tools",
    "autoload": {
        "psr-4": {
            "Ibuildings\\QaTools\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ibuildings\\QaTools\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Ibuildings\\QaTools\\Composer\\PharScriptHandler::requireBoxPhar"
        ],
        "build": [
            "Ibuildings\\QaTools\\Composer\\PharScriptHandler::buildPhar"
        ]
    },
    "require": {
        "symfony/console": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/config": "^3.0",
        "symfony/yaml": "^3.0",
        "beberlei/assert": "^2.5",
        "twig/twig": "^1.24"
    },
    "extra": {
        "qa-tools-box-source": "https://github.com/box-project/box2/releases/download/2.7.2/box-2.7.2.phar",
        "qa-tools-box-install-path": "./bin/box.phar",
        "qa-tools-box-sha-sum": "47c34f95a322e9f8cc6572696decc6473b9331c5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "mockery/mockery": "^0.9.4"
    }
}
